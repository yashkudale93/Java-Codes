import java.util.*;

public class Loops {
    public static void main(String args[]){
        int n = 4;

        //outer loop
        for(int i=1; i<=n-i+1; i++) {
            //inner looop
            for(int j=1; j<n-i+1; j++){
                System.out.print(j);
            }
            System.out.println();  
        }
    } 
}
